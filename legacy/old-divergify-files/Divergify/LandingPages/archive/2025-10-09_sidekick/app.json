{
  "expo": {
    "name": "Divergify Sidekick",
    "slug": "divergify-sidekick",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/transparentlogo.png",
    "splash": {
      "image": "./assets/transparentlogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#061a20"
    },
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/transparentlogo.png",
        "backgroundColor": "#061a20"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/transparentlogo.png"
    }
  }
}
